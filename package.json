{
  "name": "full-stack-js-boilerplate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.7.x",
    "yarn": "1.2.1"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "2.0.0",
    "reactstrap": "^5.0.0-alpha.3"
  },
  "scripts": {
    "start": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
    "server:dev": "nodemon -e js,jsx --ignore src --ignore lib --ignore build --exec babel-node server/index.js",
    "prod:build": "rimraf lib build && yarn client:build",
    "prod:start": "cross-env NODE_ENV=production heroku local",
    "client:dev": "react-scripts start",
    "client:build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "heroku-postbuild": "yarn prod:build",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "rimraf": "^2.6.2"
  }
}